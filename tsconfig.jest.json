{
    "compilerOptions": {
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "module": "ESNext",
        "target": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "types": ["jest", "node"]
    }
}
